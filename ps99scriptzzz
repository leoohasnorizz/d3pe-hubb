local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("D3pe hub", "DarkTheme")






 --- Main
 local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")






MainSection:NewButton("Dupe gems!", "dupes the bank gems", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/wxkFHQUs", true))()
end)


MainSection:NewToggle("trade lag", "trade lag", function(state)
    if state then
        loadstring(game:HttpGetAsync("https://pastebin.com/raw/wxkFHQUs", true))()("Toggle On")
    else
        loadstring(game:HttpGetAsync("https://pastebin.com/raw/wxkFHQUs", true))()("Toggle Off")
    end
end)



MainSection:NewButton("unlock all boardsHoverboards", "gives you all the hoverboards", function()
   https://pastebin.com/raw/wxkFHQUs
